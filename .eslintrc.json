{
    "parser": "@babel/eslint-parser",
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "require-jsdoc": 0,
        "max-len": ["error", { "code": 120 }]
    }
}
